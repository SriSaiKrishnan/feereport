<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>feereport</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>com.fee.report.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/loginfee</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>AddAccountant</servlet-name>
  <servlet-class>com.fee.report.controller.AddAccountantController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>AddAccountant</servlet-name>
  <url-pattern>/AddAccountant</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>AddStudent</servlet-name>
  <servlet-class>com.fee.report.controller.AddStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>AddStudent</servlet-name>
  <url-pattern>/addstudent</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>EditStudent</servlet-name>
  <servlet-class>com.fee.report.controller.EditStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>EditStudent</servlet-name>
  <url-pattern>/editstudent</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>Logout</servlet-name>
  <servlet-class>com.fee.report.controller.LogoutController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Logout</servlet-name>
  <url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>